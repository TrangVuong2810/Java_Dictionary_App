<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" stylesheets="@../stylesheet/homescreen.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.HomescreenController">
   <left>
      <VBox fx:id="navPane" prefWidth="75.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets />
               </VBox.margin></Region>
            <Button fx:id="lookupButton" alignment="CENTER" contentDisplay="CENTER" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#setLookupScreen" prefHeight="75.0" prefWidth="75.0" VBox.vgrow="ALWAYS">
               <graphic>
                  <ImageView fitHeight="70.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../media/search.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="historyButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#setHistoryScene" prefHeight="75.0" prefWidth="75.0">
               <graphic>
                  <ImageView fitHeight="70.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../media/clock.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="gameButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#setGameScene" prefHeight="75.0" prefWidth="75.0">
               <graphic>
                  <ImageView fitHeight="70.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../media/gamepad.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="bookmarkButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#setBookmarkScene" prefHeight="75.0" prefWidth="75.0">
               <graphic>
                  <ImageView fitHeight="70.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../media/star.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="paraTransButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#setParTransScene" prefHeight="75.0" prefWidth="75.0" text="//gg trans">
               <graphic>
                  <ImageView fitHeight="70.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../media/document-signed.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
         <BorderPane.margin>
            <Insets right="10.0" />
         </BorderPane.margin>
      </VBox>
   </left>
   <center>
      <StackPane fx:id="mainPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <children>
            <Label contentDisplay="CENTER" text="WELCOME TO THE DICTIONARY APPLICATION" textAlignment="CENTER" StackPane.alignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
      </StackPane>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
