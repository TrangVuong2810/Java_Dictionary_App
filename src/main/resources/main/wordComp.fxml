<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="wordComp" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.WordCompController">
   <children>
      <HBox spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <Region minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="50.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="bookmarkButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#bookmarkWord" prefHeight="50.0" prefWidth="50.0" text="//b" textAlignment="CENTER" />
            <Button fx:id="editButton" ellipsisString="E..." minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#editWord" prefHeight="50.0" prefWidth="50.0" text="//edit" textAlignment="CENTER" />
            <Button fx:id="deleteButton" alignment="CENTER" ellipsisString="D..." minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#deleteWord" prefHeight="50.0" prefWidth="50.0" text="//del" textAlignment="CENTER" />
         </children>
      </HBox>
      <Label fx:id="wordLabel" text="word" />
      <HBox prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="pronunButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="//pronun" />
            <TextField fx:id="ipaLabel" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <VBox VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="wordTypeLabel" text="//type" VBox.vgrow="ALWAYS" />
            <TextArea fx:id="wordMeaning" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
