<BorderPane minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomescreenController">
   <left>
      <VBox prefWidth="85.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="lookupButton" alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#setLookupScreen" prefHeight="75.0" prefWidth="75.0" text="//lookup" />
            <Button fx:id="historyButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#setHistoryScene" prefHeight="75.0" prefWidth="75.0" text="//history" />
            <Button fx:id="gameButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#setGameScene" prefHeight="75.0" prefWidth="75.0" text="//game" />
