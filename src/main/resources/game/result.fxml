<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="500.0" prefWidth="600.0" stylesheets="@../stylesheet/result.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.Result">
   <children>
      <VBox alignment="CENTER">
         <children>
            <HBox fx:id="congratsBox" alignment="CENTER" prefHeight="177.0" prefWidth="560.0">
               <children>
                  <Label fx:id="congratsLabel" alignment="CENTER" text="CONGRATULATIONS" textAlignment="CENTER" />
               </children>
            </HBox>
            <HBox fx:id="resultBox" prefHeight="287.0" prefWidth="540.0" spacing="30.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="250.0" spacing="50.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Label fx:id="corrects" prefWidth="200.0" styleClass="normal" text="Score:" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                           <padding>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </padding>
                        </Label>
                        <Label fx:id="totalTimer" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" styleClass="normal" text="Total Timer" VBox.vgrow="ALWAYS">
                           <padding>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </padding></Label>
                     </children>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </VBox>
                  <PieChart fx:id="pieChart" prefHeight="275.0" prefWidth="266.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
