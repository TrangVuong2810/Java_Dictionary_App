<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane maxHeight="400.0" maxWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.Game">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox prefHeight="40.0" prefWidth="600.0" spacing="10.0">
               <children>
                  <Region minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="ALWAYS" />
                  <Label fx:id="questionCount" minHeight="-Infinity" minWidth="-Infinity" prefHeight="17.0" prefWidth="100.0" text="Question: " />
                  <Label fx:id="timeLabel" minHeight="-Infinity" minWidth="-Infinity" prefHeight="17.0" prefWidth="67.0" text="00:00" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox fx:id="QuestionBox" prefHeight="303.0" prefWidth="580.0" VBox.vgrow="ALWAYS" />
            <HBox prefHeight="38.0" prefWidth="580.0" VBox.vgrow="ALWAYS">
               <children>
                  <Region minHeight="-Infinity" minWidth="-Infinity" prefHeight="20.0" prefWidth="500.0" />
                  <Button fx:id="nextBtn" mnemonicParsing="false" onAction="#loadNextQuestion" prefHeight="39.0" prefWidth="73.0" text="Next" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
